{
  "name": "generator-sub",
  "version": "2.0.2",
  "description": "A Yeoman generator for writing cli apps inspired by 37signals sub",
  "homepage": "http://github.com/prekolna/generator-sub",
  "author": {
    "name": "Doug Wade",
    "email": "doug@dougwade.io",
    "url": "dougwade.io"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "chalk": "^2.3.2",
    "inquirer": "^6.0.0",
    "yeoman-generator": "^2.0.0",
    "yosay": "2.0.2"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "nyc": "^12.0.1",
    "xo": "^0.21.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.7.0"
  },
  "repository": "prekolna/generator-sub",
  "scripts": {
    "lint": "xo generators/app/index.js generators/command/index.js test/*.js",
    "test": "npm run unit-test && npm run lint",
    "unit-test": "ava"
  },
  "license": "MIT",
  "xo": {
    "rules": {
      "key-spacing": [
        "error",
        {
          "align": "colon"
        }
      ],
      "babel/object-shorthand": 0
    },
    "esnext": true
  }
}
